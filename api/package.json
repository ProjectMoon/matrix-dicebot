{
  "name": "grpc-web-simple-example",
  "version": "0.1.0",
  "description": "gRPC-Web simple example",
  "scripts": {
    "build:protobuf": "mkdir -p ./_proto && protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts  -I ../rpc/protos --js_out=import_style=commonjs,binary:./_proto --ts_out=service=grpc-web:./_proto ../rpc/protos/*.proto",
    "build:webpack": "webpack"
  },
  "main": "server.js",
  "dependencies": {
    "@improbable-eng/grpc-web": "^0.13.0",
    "google-protobuf": "^3.14.0",
    "jsx-dom": "^7.0.0-beta.0"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.7.4",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^7.1.2",
    "html-webpack-plugin": "^4.5.0",
    "ts-loader": "^8.0.13",
    "ts-protoc-gen": "^0.14.0",
    "typescript": "^4.1.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1"
  }
}
